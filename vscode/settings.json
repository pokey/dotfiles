{
  "[handlebars]": {
    "editor.formatOnSave": false
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[java]": {
    "editor.defaultFormatter": "mwpb.java-prettier-formatter"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.formatOnType": true
  },
  "[scm]": {
    "editor.defaultFormatter": "AndreasArvidsson.andreas-talon",
    "editor.colorDecorators": false
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[talon]": {
    "editor.defaultFormatter": "wenkokke.talonfmt-vscode",
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": ["source.fixAll.format", "source.fixAll.eslint"]
  },
  "[typescriptreact]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": ["source.fixAll.format", "source.fixAll.eslint"]
  },
  "breadcrumbs.enabled": false,
  "cSpell.showStatus": false,
  "cSpell.userWords": [
    "cursorless",
    "deserialization",
    "esbuild",
    "itertools",
    "jsonl",
    "nocheck",
    "outfile",
    "subpiece",
    "subtoken",
    "Subtokens",
    "subwords",
    "typer",
    "unmount"
  ],
  "css.validate": false,
  "cursorless.colors.dark": {
    // "green": "#439932", // T
    // "green": "#43BA17", // pokey
  },
  "cursorless.colors.light": {
    "default": "#757180",
    "blue": "#095ded",
    "green": "#36B33F",
    "red": "#E02D28",
    "pink": "#e0679f",
    "yellow": "#edb62b",
    "userColor1": "#6a00ff",
    "userColor2": "#ffd8b1"
  },
  "cursorless.debug": true,
  "cursorless.experimental.keyboard.modal.keybindings.actions": {
    "t": "setSelection",
    "h": "setSelectionBefore",
    "l": "setSelectionAfter",
    "O": "editNewLineBefore",
    "o": "editNewLineAfter",
    "k": "insertCopyBefore",
    "j": "insertCopyAfter",
    "u": "replaceWithTarget",
    "m": "moveToTarget",
    "c": "clearAndSetSelection",
    "as": "swapTargets",
    "af": "foldRegion",
    "ak": "insertEmptyLineBefore",
    "aj": "insertEmptyLineAfter",
    "ai": "insertEmptyLinesAround",
    "ac": "copyToClipboard",
    "ax": "cutToClipboard",
    "ap": "pasteFromClipboard",
    "ad": "followLink"
  },
  "cursorless.experimental.keyboard.modal.keybindings.colors": {
    "d": "default",
    "b": "blue",
    "g": "yellow",
    "r": "red"
  },
  "cursorless.experimental.keyboard.modal.keybindings.scopes": {
    "i": "line",
    "p": "paragraph",
    "n": "statement",
    ",": "collectionItem",
    ".": "functionCall",
    "'": "string",
    "sf": "namedFunction",
    "sc": "class",
    "st": "token",
    "sy": "type",
    "sv": "value",
    "sk": "collectionKey",
    "sp": "nonWhitespaceSequence",
    "sa": "argumentOrParameter"
  },
  "cursorless.experimental.keyboard.modal.keybindings.shapes": {
    "x": "ex",
    "f": "fox",
    "q": "frame",
    "v": "curve",
    "e": "eye",
    "y": "play",
    "z": "bolt",
    "w": "crosshairs"
  },
  "cursorless.experimental.snippetsDir": "/Users/pokey/pokey-home-files/cursorless-snippets",
  "cursorless.hatEnablement.colors": {
    "green": false,
    "pink": false
  },
  "cursorless.hatEnablement.shapes": {
    "ex": true,
    "fox": true,
    "wing": true,
    "hole": true,
    "frame": true,
    "curve": true,
    "eye": true,
    "play": true,
    "bolt": true,
    "crosshairs": true
  },
  "cursorless.hatPenalties.colors": {
    "yellow": 1.5
  },
  "cursorless.hatPenalties.shapes": {
    "eye": 2
  },
  "cursorless.hatVerticalOffset": 10,
  "cursorless.individualHatAdjustments": {
    "default": {
      "sizeAdjustment": -8,
      "verticalOffset": 0.35
    },
    "ex": {
      "sizeAdjustment": -26,
      "verticalOffset": 0
    },
    "fox": {
      "sizeAdjustment": -15,
      "verticalOffset": 1
    },
    "wing": {
      "sizeAdjustment": -15.5,
      "verticalOffset": 0
    },
    "hole": {
      "sizeAdjustment": -32,
      "verticalOffset": 1
    },
    "frame": {
      "sizeAdjustment": 0,
      "verticalOffset": 1
    },
    "curve": {
      "sizeAdjustment": 5,
      "verticalOffset": 1
    },
    "eye": {
      "sizeAdjustment": 6.5,
      "verticalOffset": 0.5
    },
    "play": {
      "sizeAdjustment": 8,
      "verticalOffset": 2
    },
    "bolt": {
      "sizeAdjustment": -22.5,
      "verticalOffset": 0
    },
    "crosshairs": {
      "sizeAdjustment": -35,
      "verticalOffset": 0.5
    }
  },
  "cursorless.keyboard.relinquishCursorControlOnDeactivate": true,
  "cursorless.showAdditionalHighlightBeforeScroll": false,
  "cursorless.showOnStart": true,
  // WARNING: The following setting is undocumented and may disappear at any time.
  "cursorless.private.hatShapesDir": "/Users/pokey/pokey-home-files/cursorless-hat-shapes",
  "customizeUI.activityBar": "regular",
  "customizeUI.stylesheet": {
    ".title-actions:hover .actions-container": "display: flex !important;",
    ".title-actions .actions-container": "display: none !important;"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.codeLensFontSize": 10,
  "editor.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.inlayHints.fontSize": 10,
  "editor.inlineSuggest.enabled": true,
  "editor.lineDecorationsWidth": 0, // undocumented
  "editor.lineNumbers": "off",
  "editor.lineNumbersMinChars": 0,
  "editor.minimap.enabled": false,
  "editor.minimap.scale": 3,
  "editor.padding.top": 2,
  "editor.quickSuggestions": {
    // Controls whether suggestions should automatically show up while typing.
    // "other": true,
    "comments": true,
    "strings": true
  },
  "editor.scrollbar.vertical": "visible",
  "editor.suggest.localityBonus": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Abyss]": {
      "strings": "#15cc24",
      "keywords": "#be0000",
      "variables": "#587fcc",
      "comments": "#00950c",
      "textMateRules": [
        {
          "scope": "support.type",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "keyword.operator",
          "settings": {
            "foreground": "#bdbdbd"
          }
        },
        {
          "scope": "keyword.operator.logical.python",
          "settings": {
            "foreground": "#BE0000"
          }
        },
        {
          "scope": "keyword.operator.new",
          "settings": {
            "foreground": "#BE0000"
          }
        },
        {
          "scope": "storage.type",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "variable.parameter",
          "settings": {
            "foreground": "#7465ff",
            "fontStyle": ""
          }
        },
        {
          "scope": "support.class",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "entity.name.tag",
          "settings": {
            "foreground": "#33bed6"
          }
        },
        {
          "scope": "entity.name.type",
          "settings": {
            "foreground": "#e7e29e"
          }
        },
        {
          "scope": "variable.language.this",
          "settings": {
            "foreground": "#65b3e0"
          }
        }
      ]
    }
  },
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "rc": "shellscript",
    "Fastfile": "ruby"
  },
  "files.defaultLanguage": "markdown",
  "files.refactoring.autoSave": true,
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "git-branchless.branchPrefix": "pokey/",
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.mergeEditor": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "githubIssues.issueBranchTitle": "${user}/issue${issueNumber}-${sanitizedIssueTitle}",
  "githubPullRequests.defaultMergeMethod": "squash",
  "githubPullRequests.fileListLayout": "tree",
  "gitlens.advanced.fileHistoryFollowsRenames": true,
  "gitlens.codeLens.scopes": ["document"],
  "isort.args": ["--profile", "black"],
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jupyter.alwaysTrustNotebooks": true,
  "jupyter.askForKernelRestart": false,
  "jupyter.enableNativeInteractiveWindow": true,
  "markdown.extension.toc.levels": "2..6",
  "markdown.updateLinksOnFileMove.enabled": "always",
  "modalkeys.normalCursorStyle": "line-thin",
  "modalkeys.startInNormalMode": false,
  "multiCommand.commands": [
    {
      "command": "multiCommand.exitAndSave",
      "sequence": [
        "vscode-neovim.escape",
        "vscode-neovim.enable",
        "workbench.action.files.save"
      ]
    },
    {
      "command": "multiCommand.commandMode",
      "sequence": ["vscode-neovim.escape", "vscode-neovim.enable"]
    },
    {
      "command": "multiCommand.neovim-to-cursorless",
      "interval": 30,
      "sequence": [
        {
          "command": "type",
          "args": {
            "text": "i"
          }
        },
        "vscode-neovim.disable",
        "cursorless.keyboard.modal.modeOn"
      ]
    },
    {
      "command": "multiCommand.cursorless-to-neovim",
      "interval": 30,
      "sequence": [
        "cursorless.keyboard.modal.modeOff",
        "vscode-neovim.escape",
        "vscode-neovim.enable"
      ]
    },
    {
      "command": "multiCommand.cursorless-to-neovim-and-save",
      "interval": 30,
      "sequence": [
        "cursorless.keyboard.modal.modeOff",
        "vscode-neovim.escape",
        "vscode-neovim.enable",
        "workbench.action.files.save"
      ]
    }
  ],
  "notebook.cellToolbarLocation": {
    "jupyter-notebook": "left"
  },
  "notebook.globalToolbar": false,
  "notebook.showCellStatusBar": "visibleAfterExecute",
  "notebook.undoRedoPerCell": false,
  "nxConsole.enableTelemetry": false,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.indexing": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.languageServer": "Pylance",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingModuleSource": "none"
  },
  "references.preferredLocation": "view",
  "ruby.useBundler": true, //run non-lint commands with bundle exec
  "ruby.useLanguageServer": true, // use the internal language server (see below)
  "ruby.lint": {
    "rubocop": {
      "useBundler": true // enable rubocop via bundler
    },
    "reek": {
      "useBundler": true // enable reek via bundler
    }
  },
  "ruby.format": "rubocop", // use rubocop for formatting
  // "search.mode": "reuseEditor",
  "tabnine.experimentalAutoImports": true,
  "tailwindCSS.emmetCompletions": true,
  "terminal.integrated.commandsToSkipShell": [
    // Ensure the toggle sidebar visibility keybinding skips the shell
    "command-server.runCommand",
    "command-server.updateCoreState"
  ],
  "terminal.integrated.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.shellIntegration.enabled": true,
  "typeChallenges.workspaceFolder": "/Users/pokey/.typeChallenges",
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.suggest.completeFunctionCalls": true,
  // "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vscode-neovim.logLevel": "debug",
  "vscode-neovim.logOutputToConsole": true,
  "vscode-neovim.neovimExecutablePaths.darwin": "/Users/pokey/nvim-osx64/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "/Users/pokey/.config/nvim/vscode-init.vim",
  "vscode-neovim.textDecorationsAtTop": true,
  "vsicons.dontShowNewVersionMessage": true,
  "vsintellicode.features.python.deepLearning": "enabled",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.nativeFullScreen": false,
  "window.newWindowDimensions": "fullscreen",
  "window.openFoldersInNewWindow": "on",
  "window.restoreFullscreen": true,
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "[Abyss]": {
      "editorError.foreground": "#b32222",
      "editor.background": "#000000",
      "statusBar.foreground": "#767a7e",
      "editor.foreground": "#bec0c4",
      "diffEditor.insertedTextBackground": "#00cf341c",
      "editor.wordHighlightBackground": "#ffffff2b",
      "editor.wordHighlightStrongBackground": "#ffffff2b",
      "editor.rangeHighlightBackground": "#ffffff2b",
      "editor.selectionHighlightBackground": "#ffffff2b",
      "cursorless.pendingDeleteBackground": "#ff00008a",
      "cursorless.referencedBackground": "#00a2ff4d",
      "cursorless.justAddedBackground": "#09ff005b",
      "cursorless.pendingModification0Background": "#8c00ff86",
      "cursorless.pendingModification1Background": "#ff009d7e",
      "cursorless.highlight0Background": "#ca1bff61"
      // "cursorless.scopeDomainBackground": "#00e1ff18"
      // "cursorless.highlight1Background": "#49c8ff83"
      // "cursorless.highlight0Outline": "#c919ffb2",
    }
  },
  "workbench.colorTheme": "Abyss",
  "workbench.editor.enablePreview": true,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.preferredDarkColorTheme": "Abyss",
  "redhat.telemetry.enabled": false,
  "latex-workshop.mathpreviewpanel.editorGroup": "right",
  "latex-workshop.view.pdf.color.dark.pageColorsBackground": "#000000",
  "latex-workshop.view.pdf.color.dark.pageColorsForeground": "#AEAEAE",
  "latex-workshop.view.pdf.color.dark.backgroundColor": "#000000",
  "application.shellEnvironmentResolutionTimeout": 20,
  "terminal.integrated.shellIntegration.history": 1000,
  "editor.renderWhitespace": "boundary",
  "editor.defaultColorDecorators": true,
  "talon-filetree.letterStyling": "uppercase",
  "workbench.editor.pinnedTabSizing": "shrink",
  "editor.lineHeight": 1.6,
  "cursorless.hatSizeAdjustment": 70,
  "githubPullRequests.pullBranch": "never",
  "markdown.editor.pasteUrlAsFormattedLink.enabled": "smart",
  "window.title": "${activeEditorShort}${separator}${rootName}${separator}${profileName}${separator}focus:[${focusedView}]",
  "window.zoomLevel": 1
}
