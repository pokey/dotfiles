{
    "command-server.backgroundWindowProtection": true,
    "git.mergeEditor": false,
    "files.refactoring.autoSave": true,
    "editor.padding.top": 2,
    "files.defaultLanguage": "markdown",
    "editor.suggestSelection": "first",
    "modalkeys.startInNormalMode": false,
    "modalkeys.normalCursorStyle": "line-thin",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vscode-neovim.neovimExecutablePaths.darwin": "/Users/pokey/nvim-osx64/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.darwin": "/Users/pokey/.config/nvim/vscode-init.vim",
    "css.validate": false,
    "multiCommand.commands": [
        {
            "command": "multiCommand.exitAndSave",
            "sequence": [
                "vscode-neovim.escape",
                "vscode-neovim.enable",
                "workbench.action.files.save",
            ]
        },
        {
            "command": "multiCommand.commandMode",
            "sequence": [
                "vscode-neovim.escape",
                "vscode-neovim.enable",
            ]
        },
        {
            "command": "multiCommand.neovim-to-cursorless",
            "interval": 30,
            "sequence": [
                {
                    "command": "type",
                    "args": {
                        "text": "i"
                    }
                },
                "vscode-neovim.disable",
                "cursorless.keyboard.modal.modeOn"
            ]
        },
        {
            "command": "multiCommand.cursorless-to-neovim",
            "interval": 30,
            "sequence": [
                "cursorless.keyboard.modal.modeOff",
                "vscode-neovim.escape",
                "vscode-neovim.enable",
            ]
        },
        {
            "command": "multiCommand.cursorless-to-neovim-and-save",
            "interval": 30,
            "sequence": [
                "cursorless.keyboard.modal.modeOff",
                "vscode-neovim.escape",
                "vscode-neovim.enable",
                "workbench.action.files.save",
            ]
        }
    ],
    "terminal.integrated.commandsToSkipShell": [
        // Ensure the toggle sidebar visibility keybinding skips the shell
        "command-server.runCommand",
        "command-server.updateCoreState",
    ],
    "terminal.integrated.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
